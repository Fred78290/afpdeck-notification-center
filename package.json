{
  "name": "afpdeck-notification-center-server",
  "version": "1.0.0",
  "description": "Afpdeck Notification Center for NodeJS",
  "main": "dist/index.js",
  "repository": "https://github.com/Fred78290/afpdeck-notification-center",
  "author": "Fred78290",
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon --watch lambda -r dotenv/config -e js,ts,json --exec \"ts-node src/index.ts\"",
    "build": "tsup src/index.ts --format cjs --clean",
    "docker": "./docker.sh",
    "local": "node dist/index.js -r dotenv/config",
    "start": "node index.js",
    "compile": "tsc --outDir build",
    "unit": "jest --runInBand",
    "test": "jest --runInBand --detectOpenHandles"
  },
  "dependencies": {
    "@aws-sdk/lib-dynamodb": "^3.441.0",
    "@hexlabs/dynamo-ts": "^4.0.155",
    "afp-apicore-sdk": "^1.1.12",
    "auth-header": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.5",
    "express": "^4.18.2",
    "install": "^0.13.0",
    "mongoose": "^8.0.1",
    "nodejs-base64": "^2.0.0",
    "npm": "^10.2.2",
    "web-push": "^3.6.6"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.454.0",
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@jest/globals": "^29.7.0",
    "@swc/core": "^1.3.99",
    "@types/auth-header": "^1.0.6",
    "@types/aws-lambda": "^8.10.129",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-useragent": "^1.0.5",
    "@types/jest": "^29.5.10",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@types/web-push": "^3.6.3",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  },
  "lib": [
    "esnext"
  ]
